local Database = loadstring(game:HttpGet("https://pastebin.com/raw/7iJNJ6L0"))()
local HWID

local function Generate()
    -- Universal identifier without HashService
    local components = {
        game:GetService("Players").LocalPlayer.UserId,
        game.PlaceId,
        tostring(tick()),
        math.random(1, 1e9),
        game:GetService("HttpService"):GenerateGUID(false)
    }
    return string.gsub(table.concat(components, "|"), "%W", "") -- Remove non-alphanumeric chars
end

local function Validate(key)
    HWID = Generate()
    
    if not Database[key] then
        game.Players.LocalPlayer:Kick("‚ùå Invalid Key")
        return false
    end
    
    if Database[key] == "" then
        writefile(key..".hwid", HWID)
        syn.request({
            Url = "https://discord.com/api/webhooks/1355522134465577160/YTDmXZIdP748QHq8K21k0CHJgPbKqY_zbYQQqfbwnsQ11MlJMMQuIVKz2b2FT_kiC_Rm",
            Method = "POST",
            Body = "Key="..key.."&HWID="..HWID
        })
        return true
    end
    
    local valid = isfile(key..".hwid") and readfile(key..".hwid") == HWID
    if not valid then
        game.Players.LocalPlayer:Kick("‚ö†Ô∏è HWID Changed")
    end
    return valid
end

if _G.key and Validate(_G.key) then
    print("‚úÖ Whitelisted")
    -- Load your script here
else
    game.Players.LocalPlayer:Kick("üö´ Access Denied")
end
